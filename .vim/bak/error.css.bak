/* ALERTS */
.alert {
  padding: 5px 30px;
  background-repeat: no-repeat;
  background-position: 5px center;
  border-radius: 0;
  position: fixed;
  top: 30px;
  left: 50%;
  z-index: 10;
  display: none;
  -webkit-animation: fade_out 0.5s ease 8s forwards;
  animation: fade_out 0.5s ease 8s forwards; }
  .alert.alert-info {
    background-color: #f1f1f1;
    border-color: #f1f1f1;
    color: #555555; }
  .alert.alert-success {
    background-color: #f1f1f1;
    border-color: #cccccc;
    background-image: url('../img/white/icons/success-check.png');
    color: #555555; }
  .alert .close {
    opacity: 0.5;
    color: #000000; }

