/* blue button */
.btn-primary {
  background-color: #5daf9a;
  border-color: #5daf9a;
  color: #ffffff; }
  .btn-primary:hover, .btn-primary:active, .btn-primary:focus {
    background-color: #338e74;
    border-color: #338e74; }
