{% from 'macros.jinja2' import basic_field, checkbox_field, append_field, prepend_field %}

<div class="row">
  <div class="col-md-5">{{ basic_field(form.shade.form.type) }}</div>
  <div class="col-md-5">{{ append_field(form.shade.height, 'in') }}</div>
</div>
<div class="row">
  <div class="col-md-5">{{ append_field(form.shade.width, 'in') }}</div>
  <div class="col-md-5">{{ append_field(form.shade.depth, 'in') }}</div>
</div>
<div class="row">
  <div class="col-md-5">{{ append_field(form.shade.diameter, 'in') }}</div>
  <div class="col-md-5">{{ basic_field(form.shade.material) }}</div>
</div>
<div class="row">
  <div class="col-md-5">{{ basic_field(form.shade.quantity) }}</div>
</div>
